{
  "name": "opentsdb",
  "version": "0.4.0",
  "description": "OpenTSDB client library.",
  "author": {
    "name": "Athan Reines",
    "email": "athan@nodeprime.com"
  },
  "contributors": [
    {}
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "./lib",
  "repository": {
    "type": "git",
    "url": "git://github.com/kgryte/opentsdb.js.git"
  },
  "bugs": {
    "url": "https://github.com/kgryte/opentsdb.js/issues"
  },
  "keywords": [
    "opentsdb",
    "tsdb",
    "timeseries",
    "query",
    "poll"
  ],
  "dependencies": {
    "request": "^2.37.0"
  },
  "devDependencies": {
    "chai": "1.x.x",
    "coveralls": "^2.11.1",
    "express": "^4.6.1",
    "istanbul": "^0.3.0",
    "mocha": "1.x.x"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
